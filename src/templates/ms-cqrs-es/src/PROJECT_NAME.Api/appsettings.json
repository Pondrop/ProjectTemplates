{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CosmosConfiguration": {
    "DatabaseName": "COSMOS_DB_NAME",
    "ApplicationName": "COSMOS_DB_APP_NAME",
    "ConnectionString": "COSMOS_DB_CONNECTION_STRING"
  },
  "DaprEventTopicConfiguration" : {
    "UserUpdateConfiguration": {
      "AppId": "USER_UPDATE_APP_ID",
      "MethodName": "USER_UPDATE_METHOD_NAME",
      "EventTopic": "USER_UPDATE_EVENT_TOPIC"
    }
  }
}
